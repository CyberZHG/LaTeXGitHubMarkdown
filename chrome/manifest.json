{
  "manifest_version": 2,

  "name": "LaTeX GitHub Markdown",
  "description": "Show LaTeX formulas for GitHub Markdown files.",
  "version": "0.1.0",

  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "webNavigation",
    "https://github.com/",
    "https://gist.github.com/",
    "https://cyberzhg.github.io/"
  ],
  "background": {
    "scripts": ["inject.js"]
  }
}
